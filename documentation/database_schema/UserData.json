{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "UserData",
    "properties": {
        "firstName": {
            "type": "string"
        },
        "lastName": {
            "type": "string"
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "logDate": {
            "type": "string",
            "format": "date-time"
        },
        "validUntil": {
            "type": "string",
            "format": "date-time"
        }
    },
    "additionalProperties": false,
    "required": [
        "firstName",
        "lastName",
        "email",
        "logDate",
        "validUntil"
    ]
}