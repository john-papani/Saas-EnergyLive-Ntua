{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "AggregatedGenerationPerType",
    "properties": {
        "timestamp": {
            "type": "string",
            "format": "date-time"
        },
        "mapCode": {
            "type": "string",
            "minLength": 2,
            "maxLength": 2
        },
        "generation": {
            "type": "number"
        },
        "productionType": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "timestamp",
        "mapCode",
        "generation",
        "productionType"
    ]
}